{
  "name": "@winter-love/signals",
  "version": "0.0.1",
  "repository": "git@github.com:bichikim/web-react.git",
  "author": "bihci@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/preact.mjs",
      "require": "./dist/preact.js"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./*": "./*"
  },
  "main": "dist/react.js",
  "module": "dist/react.mjs",
  "source": "src/react.ts",
  "types": "dist/react.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts && tsup --dts --config tsup.preact.config.ts",
    "publish": "npm publish"
  },
  "dependencies": {
    "@preact/signals-react": "^1.1.1",
    "@winter-love/signals-rebuild": "workspace:*"
  },
  "devDependencies": {
    "esbuild-plugin-replace": "^1.2.0",
    "esbuild-plugin-alias": "^0.2.1",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.14",
    "flush-promises": "^1.0.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "preact": "^10.11.1",
    "react-dom": "^18",
    "tslib": "^2.4.0",
    "tsup": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
