{
  "name": "@winter-love/react-componnets",
  "version": "0.0.1",
  "repository": "https://github.com/bluchip-nft/bluchip",
  "author": "bihci@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts",
    "publish": "npm publish"
  },
  "dependencies": {
    "@winter-love/js-utils": "^0.0.1",
    "react": "^18.2",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.14",
    "lodash": "^4.17.21",
    "react-dom": "^18",
    "tslib": "^2.4.0",
    "tsup": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  },
  "gitHead": "789a3f185c83ee613e500614dad679fa8eb9b33f"
}
