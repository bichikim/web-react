{
  "name": "@winter-love/react-hooks",
  "version": "0.0.2-y.0",
  "repository": "git@github.com:bichikim/web-react.git",
  "author": "bihci@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts"
  },
  "dependencies": {
    "@winter-love/utils": "^1.0.27-alpha.9",
    "immer": "^9.0.15",
    "react": "^18.2",
    "react-fast-compare": "^3.2.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.14",
    "flush-promises": "^1.0.2",
    "lodash": "^4.17.21",
    "react-dom": "^18",
    "tslib": "^2.4.0",
    "tsup": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1a40cb6a97ad084c8d9244e74cc3fefdca03504f",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
