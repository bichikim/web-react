{
  "name": "@bichikim/utils",
  "version": "0.0.1",
  "author": "bichi@live.co.kr",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup --dts --minify",
    "dev": "tsup --watch",
    "prepare": "tsup --dts",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.9",
    "lodash": "^4.17.21",
    "tsd": "^0.22.0",
    "tsup": "^6.2.3",
    "typescript": "^4.6.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "gitHead": "789a3f185c83ee613e500614dad679fa8eb9b33f",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm",
      "iife"
    ],
    "outDir": "dist",
    "sourcemap": true,
    "splitting": false
  }
}
